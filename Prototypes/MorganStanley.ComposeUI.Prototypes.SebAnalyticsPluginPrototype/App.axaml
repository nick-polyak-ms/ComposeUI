<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:np="https://np.com/visuals"
             x:Class="MorganStanley.ComposeUI.Prototypes.SebAnaylicsPluginPrototype.App">
	<Application.Styles>
		<StyleInclude Source="avares://Avalonia.Themes.Default/Accents/BaseDark.xaml"/>
		<StyleInclude Source="avares://Avalonia.Themes.Default/DefaultTheme.xaml"/>
		<StyleInclude Source="avares://Avalonia.Controls.DataGrid/Themes/Default.xaml"/>
		<StyleInclude Source="avares://NP.Avalonia.Visuals/Themes/CustomWindowStyles.axaml"/>
		<StyleInclude Source="avares://NP.Avalonia.UniDock/Themes/DockStyles.axaml"/>
		<StyleInclude Source="avares://NP.Avalonia.Visuals/Themes/ThemeStyles.axaml"/>
		<StyleInclude Source="avares://SebVisualsCommon/Themes/SebastionStyles.axaml"/>

		<Style Selector="np|DockItemPresenter">
			<Setter Property="HeaderBackground"
					Value="DarkGray"/>

		</Style>

		<Style Selector="np|DockItemPresenter /template/ Grid">
			<Setter Property="Background"
					Value="Black"/>

		</Style>
		<Style Selector="np|TabbedDockGroup">
			<Setter Property="Background"
					Value="Black"/>
		</Style>

	</Application.Styles>
</Application>
